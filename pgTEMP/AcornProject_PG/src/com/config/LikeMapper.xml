<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="LikeMapper">



	<!--댓글:삽입 -->
	<insert id="likeInsert" parameterType="LikeDTO">
		Insert
		INTO liked (reviewId,mbrId)
		VALUES (#{reviewId},#{mbrId})
	</insert>

	
	<!--좋아요한 리스트 가져오기-->
	<select id="reviewLikeCheck" parameterType="int" resultType="list">
		SELECT mbrId
		FROM liked
		WHERE reviewId=#{reviewId}
	</select>

	

</mapper>